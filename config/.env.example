# Server Configuration
PORT=5000
NODE_ENV=development

# Google Cloud SQL Configuration
DB_USER=your_cloud_sql_user
DB_PASSWORD=your_cloud_sql_password
DB_NAME=your_database_name
DB_HOST=/cloudsql/your-project:your-region:your-instance
DB_PORT=3306
INSTANCE_UNIX_SOCKET=/cloudsql/your-project:your-region:your-instance

# Google Cloud Project
GCLOUD_PROJECT=your-project-id
GCLOUD_REGION=your-region

# JWT Configuration
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRE=24h

# Email Configuration
EMAIL_SERVICE=smtp
EMAIL_USER=your_email@example.com
EMAIL_PASSWORD=your_email_password
FRONTEND_URL=http://localhost:3000

# AWS Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-east-1
AWS_BUCKET_USER_UPLOADS=user-uploads
AWS_BUCKET_QUESTION_BANK=question-bank
AWS_BUCKET_REPORTS=analytics-reports

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key